'.source.yaml':
  'filesystem':
      'prefix': 'filesystem'
      'body':"""
         ${1:filesystem_name}:
         	Type: AWS::EFS::FileSystem
         	Properties:
         		Encrypted: ${2:Boolean}     # optional
         		FileSystemTags:                # optional, list of ElasticFileSystemTag
         			- ${3:ElasticFileSystemTag}
         		KmsKeyId: ${4:String}     # optional
         		PerformanceMode: ${5:String}     # optional
"""