'.source.yaml':
  'basepathmapping':
      'prefix': 'basepathmapping'
      'body':"""
         ${1:basepathmapping_name}:
         	Type: AWS::ApiGateway::BasePathMapping
         	Properties:
         		BasePath: ${2:String}     # optional
         		DomainName: ${3:String}     # optional
         		RestApiId: ${4:String}     # optional
         		Stage: ${5:String}     # optional
"""