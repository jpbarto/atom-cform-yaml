         Secrets:                # optional, list of Secret
         	- ${33:Secret}
         StartTimeout: ${34:Integer}     # optional
         StopTimeout: ${35:Integer}     # optional
         SystemControls:                # optional, list of SystemControl
         	- ${36:SystemControl}
         Ulimits:                # optional, list of Ulimit
         	- ${37:Ulimit}
