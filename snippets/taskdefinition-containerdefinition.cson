         DnsServers:                # optional, list of String
         	- ${7:String}
         DockerLabels:                # optional, map of String
         	${8:String_key}: ${9:String_value}
         DockerSecurityOptions:                # optional, list of String
         	- ${10:String}
         EntryPoint:                # optional, list of String
         	- ${11:String}
         Environment:                # optional, list of KeyValuePair
         	- ${12:KeyValuePair}
         Essential: ${13:Boolean}     # optional
         ExtraHosts:                # optional, list of HostEntry
         	- ${14:HostEntry}
         FirelensConfiguration: ${15:FirelensConfiguration}     # optional
         HealthCheck: ${16:HealthCheck}     # optional
         Hostname: ${17:String}     # optional
         Image: ${18:String}     # optional
         Interactive: ${19:Boolean}     # optional
