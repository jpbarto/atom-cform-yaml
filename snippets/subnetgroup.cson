'.source.yaml':
  'subnetgroup':
      'prefix': 'subnetgroup'
      'body':"""
          ${1:subnetgroup_name}:
              Type: "AWS::ElastiCache::SubnetGroup"
              Properties:
                  CacheSubnetGroupName: ${2:String}     # optional
                  Description: ${3:String}     # required
                  SubnetIds:                # required, list of String
                      - ${4:String}
"""