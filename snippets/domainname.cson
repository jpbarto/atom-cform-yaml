'.source.yaml':
  'domainname':
      'prefix': 'domainname'
      'body':"""
         ${1:domainname_name}:
         	Type: AWS::ApiGateway::DomainName
         	Properties:
         		CertificateArn: ${2:String}     # optional
         		DomainName: ${3:String}     # required
         		EndpointConfiguration: ${4:EndpointConfiguration}     # optional
         		RegionalCertificateArn: ${5:String}     # optional
"""