'.source.yaml':
  'model':
      'prefix': 'model'
      'body':"""
         ${1:model_name}:
         	Type: "AWS::ApiGateway::Model"
         	Properties:
         		ContentType: ${2:String}     # optional
         		Description: ${3:String}     # optional
         		Name: ${4:String}     # optional
         		RestApiId: ${5:String}     # required
         		Schema: ${6:Json}     # optional
"""